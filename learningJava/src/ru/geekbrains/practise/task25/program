10 LET a = 2
20 LET b = 1
30 LET c = -5
40 LET d = b*b-4*a*c
50 IF d>0 THEN GOTO 90
60 IF d=0 THEN GOTO 160
70 PRINT "NO ROOTS"
80 GOTO 170
90 LET x1 = (-b + SQRT(d))/2/a
100 LET x2 = (-b - SQRT(d))/2/a
110 PRINT "x1=";
120 PRINT x1
130 PRINT "x2=";"x2="
140 PRINT x2
150 GOTO 170
160 PRINT "x="; -b/2/a
170 PRINT "PROGRAM FINISHED"